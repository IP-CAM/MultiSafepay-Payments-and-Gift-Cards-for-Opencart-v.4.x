                                <ul>
                                    <li>
                                        {{ text_assistance_telephone }}
                                    </li>
                                    <li>
                                        {{ text_assistance_email }}
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
